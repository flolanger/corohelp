    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
</head>
<body>
{% if not is_granted('IS_AUTHENTICATED_FULLY') %}
    <a href="{{ path('login') }}">Login</a>
{% endif %}
