    <a href="{{ path('register') }}">Register</a>
{% endif %}
{% if is_granted('IS_AUTHENTICATED_FULLY') %}
    <a href="{{ path('logout') }}">Logout</a>
{% endif %}
<h1>Welcome</h1>

<a href="{{ path('category_index') }}">Categories</a>
